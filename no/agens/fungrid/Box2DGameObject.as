package no.agens.fungrid {	import flash.display.DisplayObject;	import Box2D.Dynamics.b2Body;	import flash.geom.Point;		//	// Using Box2D V2.0	//		public class Box2DGameObject extends HardCoreGameObject{				public var body:b2Body;				public function Box2DGameObject(id:String, displayObject:DisplayObject, body:b2Body) {			super(id, displayObject, new Point(body.GetPosition().x*FunGridBox2DManager.RATIO,body.GetPosition().y*FunGridBox2DManager.RATIO));			this.body = body;			this.body.SetUserData(this);		}		/** Custom update before anything else is updated */		override protected function customEarlyUpdate():void {			pos.x = body.GetPosition().x*FunGridBox2DManager.RATIO;			pos.y = body.GetPosition().y*FunGridBox2DManager.RATIO;			displayObject.rotation = 180*body.GetAngle()/Math.PI;		}		override protected function customDestroy():void {			FunGridBox2DManager.WORLD.DestroyBody(body);		}	}	}